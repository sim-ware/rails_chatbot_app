<p id="notice"><%= notice %></p>

<p>
  <strong>Number:</strong>
  <%= @round.number %>
</p>

<p>
  <strong>Question:</strong>
  <%= @questions.find(@round.number).script %>
</p>

<p>
  <strong>Messages:</strong>
  <%= link_to @messages.find(@round.number*2-1).script, controller: "rounds", action: "show", id: @round.number+1 %>

  <%= link_to @messages.find(@round.number*2).script, fail_index_path %>
</p>
